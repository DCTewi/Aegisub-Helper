{
  "Retime": {
    "prefix": "retime",
    "body": ["!retime(\"${1:syl}\", ${2:0}, ${3:0})!"],
    "description": "retime function"
  },
  "ReLayer": {
    "prefix": "relayer",
    "body": ["!relayer(${1:0})!"],
    "description": "relayer function"
  },
  "ReStyle": {
    "prefix": "restyle",
    "body": ["!restyle(\"${1:Default}\")!"],
    "description": "restyle function"
  },
  "MaxLoop": {
    "prefix": "maxloop",
    "body": ["!maxloop(${1:1})!"],
    "description": "maxloop function"
  },
  "LoopControl": {
    "prefix": "loopctl",
    "body": ["!loopctl(${1:newj}, ${2:newmaxj})!"],
    "description": "loopctl function"
  },

  "New Template Line": {
    "prefix": "template",
    "body": [
      "%[ template ${1:syl} # ${2:comment} @ ${3:0}",
      "",
      "{",
      "\t\\pos(\\$sx, \\$sy)",
      "\t$0",
      "}",
      "",
      "%]"
    ],
    "description": "new template line"
  },

  "New Code Line": {
    "prefix": "code",
    "body": [
      "%[ code ${1:once} # ${2:comment} @ ${3:0}",
      "",
      "$0",
      "",
      "%]"
    ],
    "description": "new code line"
  }
}
